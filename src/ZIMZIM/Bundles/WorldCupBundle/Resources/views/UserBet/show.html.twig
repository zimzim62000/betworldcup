{% extends '::base.html.twig' %}


{% block title %}
    {{ 'zimzim_worldcup_userbet.index.title'|trans }}
{% endblock title %}

{% block page %}
    <div class="row">
        <div class="small-12 large-12 columns">
            <h1 class="text-center title">Entity</h1>
        </div>
    </div>
    <div class="row">
        <div class="small-12 large-12 columns">
            <table class="width-100 panel">
                <tbody>
                    <tr>
                        <th>Id</th>
                            <td>{{ entity.id }}</td>
                    </tr>
                    <tr>
                        <th>Scoreteama</th>
                            <td>{{ entity.scoreTeamA }}</td>
                    </tr>
                    <tr>
                        <th>Scoreteamb</th>
                            <td>{{ entity.scoreTeamB }}</td>
                    </tr>
                    <tr>
                        <th>Date</th>
                            <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
                    </tr>
                </tbody>
            </table>

                        <div class="row">
    <div class="small-12 large-4 columns text-center">
        <a class="tiny button" href="{{ path('zimzim_worldcup_userbet') }}">
            {{ 'link.return'|trans }}
        </a>
    </div>
    <div class="small-12 large-4 columns text-center">
        <a class="tiny button" href="{{ path('zimzim_worldcup_userbet_edit', { 'id': entity.id }) }}">
            {{ 'button.update'|trans }}
        </a>
    </div>
    <div class="small-12 large-4 columns text-center">
        {{ form(delete_form) }}
    </div>
</div>
        </div>
    </div>
{% endblock %}
